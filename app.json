{
  "expo": {
    "name": "PeerRentalApp",
    "slug": "peerrentalapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5C542"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.peerrentalapp.app",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take photos of items and for identity verification.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select photos of items.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby items and set item locations.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F5C542"
      },
      "package": "com.peerrentalapp.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "name": "ShareStash",
      "shortName": "ShareStash",
      "description": "Rent anything from people nearby",
      "themeColor": "#F5C542",
      "backgroundColor": "#F8F9FA"
    },
    "scheme": "peerrentalapp",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow PeerRentalApp to access your camera for taking item photos and identity verification."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow PeerRentalApp to use your location to show nearby items."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow PeerRentalApp to access your photos to select item images."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#F5C542"
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "afcab26b-c02b-48ce-8635-a18e36b0ae96"
      }
    },
    "owner": "jeremyjolly441"
  }
}
